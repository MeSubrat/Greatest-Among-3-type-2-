#include <stdio.h>

int main()
{
    int a,b,c;
    printf("Enter 3 numbers");
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    if(a>b){
        if(a>c){
            printf("%d is greatest",a);
        }else{
            printf("%d is greatest",c);
        }
    }else if(b>c){
        if(b>a){
            printf("%d is greatest",b);
        }else{
            printf("%d is greatest",a);
        }
    }else if(c>a){
        if(c>b){
            printf("%d is greatest",c);
        }else{
            printf("%d is greatest",b);
        }
    }else if(a==b || b==c){
        printf("All the numbers are same");
    }else{
        printf("Enter valid input");
    }
}
